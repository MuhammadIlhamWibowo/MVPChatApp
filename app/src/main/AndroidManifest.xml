<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.secdev.mvpchatapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".module.contact.add_new_contact.AddNewContactActivity"></activity>
        <activity android:name=".module.chat.message.MessageActivity" />
        <activity android:name=".module.contact.contact_list.add_new_chat.AddNewChatActivity" />
        <activity android:name=".module.contact.contact_detail.ContactDetailActivity" />
        <activity android:name=".module.splashscreen.SplashActivity" />
        <activity android:name=".module.auth.register.RegisterActivity" />
        <activity android:name=".module.auth.login.LoginActivity" />
        <activity android:name=".module.home.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>